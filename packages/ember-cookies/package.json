{
  "name": "ember-cookies",
  "version": "1.3.0",
  "description": "Cookies abstraction for Ember.js that works both in the browser as well as with Fastboot on the server.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/simplabs/ember-cookies",
  "license": "MIT",
  "author": "",
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    },
    "./test-support": {
      "types": "./declarations/test-support/index.d.ts",
      "default": "./dist/test-support/index.js"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./index.d.ts"
      ]
    }
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "copyDoc": "cp ../../*.md .",
    "lint": "concurrently \"pnpm lint:js\" \"pnpm lint:types\"",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "prepublishOnly": "concurrently \"pnpm copyDoc\" \"pnpm build\"",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'",
    "release": "npm publish"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.25.9",
    "@babel/plugin-transform-typescript": "7.27.0",
    "@embroider/addon-dev": "8.0.1",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.24.0",
    "@glint/core": "1.5.2",
    "@glint/environment-ember-loose": "1.5.2",
    "@glint/environment-ember-template-imports": "1.5.2",
    "@rollup/plugin-babel": "6.0.4",
    "@tsconfig/ember": "3.0.10",
    "@types/ember": "4.0.11",
    "@types/ember__utils": "4.0.7",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "8.29.1",
    "concurrently": "9.1.2",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-ember": "12.5.0",
    "eslint-plugin-n": "17.17.0",
    "eslint-plugin-prettier": "5.2.6",
    "globals": "16.0.0",
    "prettier": "3.5.3",
    "rollup": "4.40.0",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "ember-source": ">=4.0"
  },
  "engines": {
    "node": ">= 16.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2,
    "versionCompatibility": {
      "ember": ">=4.0"
    },
    "app-js": {
      "./services/cookies.js": "./dist/_app_/services/cookies.js"
    }
  }
}
