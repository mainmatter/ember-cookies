{
  "name": "ember-cookies",
  "version": "1.0.0",
  "description": "Cookies abstraction for Ember.js that works both in the browser as well as with Fastboot on the server.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/simplabs/ember-cookies",
  "license": "MIT",
  "author": "",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./test-support": "./dist/test-support/index.js",
    "./addon-main.js": "./addon-main.js"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "copyDoc": "cp ../../*.md .",
    "lint": "yarn lint:js",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepublishOnly": "npm-run-all --aggregate-output --continue-on-error --parallel copyDoc build",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@embroider/addon-dev": "^3.0.0",
    "@rollup/plugin-babel": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-ember": "^11.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.8.8",
    "rollup": "^2.74.1"
  },
  "engines": {
    "node": ">= 16.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "main": "addon-main.js",
    "type": "addon",
    "version": 2,
    "versionCompatibility": {
      "ember": ">=4.0"
    },
    "app-js": {
      "./services/cookies.js": "./dist/_app_/services/cookies.js"
    }
  }
}
